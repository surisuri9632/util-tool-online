{
  "version": 2,
  "builds": [
    {
      "src": "app.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "app.js"
    }
  ],
  "redirects": [
    {
      "source": "/(.*)",
      "destination": "https://util-tool-online.com",
      "permanent": true
    }
  ]
}
